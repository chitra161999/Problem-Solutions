#include <iostream>
using namespace std;
int main()
{   int n,i;
    char ch;
    cin>>n;
    for(i=0;i<n;i+=1)
    {   cin>>ch;
        switch(ch)
        {   case 'b': cout<<"BattleShip\n";
                    break;
            case 'B': cout<<"BattleShip\n";
                    break;
            case 'c': cout<<"Cruiser\n";
                    break;
            case 'C': cout<<"Cruiser\n";
                    break;
            case 'd': cout<<"Destroyer\n";
                    break;
            case 'D': cout<<"Destroyer\n";
                    break;
            case 'f': cout<<"Frigate\n";
                    break;
            case 'F': out<<"Frigate\n";
                    break;
        }  
    }  
    return 0;

}
